{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es6"
    ],
    "module": "commonjs",
    "baseUrl": "./",
    "esModuleInterop": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true
  }
}